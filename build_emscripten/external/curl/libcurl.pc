prefix=/usr/local
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include
supported_protocols="HTTP SCP SFTP WS"
supported_features="alt-svc AsynchDNS brotli HTTP2 IDN IPv6 Largefile libz threadsafe UnixSockets zstd"

Name: libcurl
URL: https://curl.se/
Description: Library to transfer files with HTTP, FTP, etc.
Version: 8.14.0-DEV
Requires: libssh2,libidn2,zlib,libbrotlidec,libbrotlicommon,libzstd,libnghttp2
Requires.private: libssh2,libidn2,zlib,libbrotlidec,libbrotlicommon,libzstd,libnghttp2
Libs: -L${libdir} -lcurl -lssh2 -lidn2 -lz -lbrotlidec -lbrotlicommon -lzstd -lnghttp2
Libs.private:  -lssh2 -lidn2 -lz -lbrotlidec -lbrotlicommon -lzstd -lnghttp2
Cflags: -I${includedir} -DCURL_STATICLIB
Cflags.private: -DCURL_STATICLIB
